{
    "name": "cepa/Beautify",
    "description": "Fork of edwinm/Beautify",
    "type": "library",
    "homepage": "https://github.com/cepa/Beautify",
    "authors": [
        {
            "name": "Edwin Martin",
            "homepage": "http://www.bitstorm.org/"
        }
    ],
    "require": {
